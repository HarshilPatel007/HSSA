<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="650.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ChartController">
   <children>
      <Pane prefHeight="97.0" prefWidth="1024.0" style="-fx-background-color: #0091ea;">
         <children>
            <Circle fx:id="btnMin" fill="#5eff03" layoutX="975.0" layoutY="18.0" onMouseClicked="#btnMinEvent" radius="12.0" stroke="WHITE" strokeType="INSIDE" strokeWidth="1.5">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Circle>
            <Circle fx:id="btnBack" fill="#ff4d05" layoutX="1004.0" layoutY="18.0" onMouseClicked="#btnBackEvent" radius="12.0" stroke="WHITE" strokeType="INSIDE" strokeWidth="1.5">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Circle>
            <Text fill="WHITE" layoutX="14.0" layoutY="59.0" stroke="BLACK" strokeLineCap="BUTT" strokeLineJoin="ROUND" strokeWidth="0.0" text="Charts">
               <font>
                  <Font name="Cantarell Extra Bold" size="30.0" />
               </font>
            </Text>
         </children>
      </Pane>
      <Button fx:id="btnShowChart" layoutX="14.0" layoutY="599.0" mnemonicParsing="false" onAction="#btnShowChartAction" text="Show Gender Data" />
      <PieChart fx:id="piechart" layoutX="10.0" layoutY="125.0" />
      <PieChart fx:id="piechart1" layoutX="514.0" layoutY="125.0" />
      <Button fx:id="btnShowChart1" layoutX="858.0" layoutY="599.0" mnemonicParsing="false" onAction="#btnShowChart1Action" text="Show Category Data" />
   </children>
</AnchorPane>
